{
    "name": "Find Artek Django Dev Container",
    "build": {
        "context": ".",
        "dockerfile": "Dockerfile"
    },
    "workspaceFolder": "/workspaces/find_artek",
    "customizations": {
        "vscode": {
            "settings": {
                "python.defaultInterpreterPath": "/workspaces/find_artek/venv/bin/python"
            },
            "extensions": [
                "ms-python.python",
                "rogalmic.bash-debug"
            ]
        }
    },
    "postCreateCommand": ""
}